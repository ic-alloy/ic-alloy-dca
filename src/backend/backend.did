type CanisterSettingsInput = record {
  asset : text;
  interval : nat64;
  owner : text;
  amount : nat64;
};
type Result = variant { Ok : text; Err : text };
service : (CanisterSettingsInput) -> {
  get_address : (opt principal) -> (Result);
  get_balance : (opt principal) -> (Result);
  send_eth : (text, nat) -> (Result);
  start : () -> (Result);
  stop : () -> (Result);
}
